{
    "containerDefinitions": [
        {
            "name": "venmito-backend",
            "image": "184994934866.dkr.ecr.us-east-1.amazonaws.com/venmito-backend:latest",
            "portMappings": [
                {
                  "containerPort": 80,
                  "protocol": "tcp"
                },
                {
                  "containerPort": 443,
                  "protocol": "tcp"
                }
              ],
            "memory": 512,
            "cpu": 256,
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/venmito-service",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "venmito-backend-task",
    "executionRoleArn": "arn:aws:iam::184994934866:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}